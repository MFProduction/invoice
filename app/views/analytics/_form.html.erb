<%= render "shared/errors", obj: @analytic %>
<div class="row">
  <div class="well col-md-12">
    <%= form_for (@analytic) do |f| %>
    <%= f.label :name  %>
    <%= f.text_field :name  %>
    <%= f.label :number  %>
    <%= f.number_field :number  %>
    <%= f.submit(@analytic.new_record? ? "Create" : "Edit", class:"btn btn-primary")  %>
    <% end  %>
    <%= link_to("Back", analytics_path, :class => "btn btn-default") %>
  </div>
</div>
