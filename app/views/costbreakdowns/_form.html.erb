<%= render "shared/errors", obj: @costbreakdown %>
<div class="row">
  <div class="well col-md-8 col-md-offset-2">
    <%= form_for @costbreakdown do |f| %>
    <%= f.label :name  %>
    <%= f.text_field :name  %>
    <%= f.label :number  %>
    <%= f.text_field :number  %>
    <%= f.submit(@costbreakdown.new_record? ? "Create" : "Edit", class:"btn btn-success")  %>
    <% end  %>
  </div>
</div>
